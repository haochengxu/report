      $(".aEqualB").show()
    }
    if (choice_test_before < choice_test_after && choice_test_after <= 7) {
      $('#less')[0].textContent= Number((Number((choice_test_after-choice_test_before)/choice_test_before).toFixed(2))).toFixed(2) * 100 + '%'
      $(".less").show()
      if (choice_test_after - choice_test_before <= 2) {
        $(".less-third").show()
      }
    }
    if (choice_test_before < choice_test_after && choice_test_after >= 8) {
      $('#more')[0].textContent= Number((Number((choice_test_after-choice_test_before)/choice_test_before).toFixed(2))).toFixed(2) * 100 + '%'
      $(".more").show()
    }
</script>

